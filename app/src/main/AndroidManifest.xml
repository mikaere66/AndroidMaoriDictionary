            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.michaelrmossman.maoridictionary.MainActivity"/>
        </activity>
        <!--suppress InjectedReferences -->
        <activity
            android:name=".SettingsActivity"
            android:label="@string/action_settings"
            android:parentActivityName="com.michaelrmossman.maoridictionary.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.michaelrmossman.maoridictionary.MainActivity"/>
        </activity>
        <!--suppress InjectedReferences -->
        <activity
            android:name=".FavouritesActivity"
            android:label="@string/activity_favourites"
